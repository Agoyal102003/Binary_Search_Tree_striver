// Approach 1->

int findCeil(Node* root, int input) {
    if (root == NULL) return -1;

    // Your code here
    int ceil=-1;
    Node* temp=root;
    while(temp!=NULL)
    {
        if(temp->data > input)
        {
            ceil=temp->data;
            temp=temp->left;
        }
        else if(temp->data <input)
        {
            temp=temp->right;
        }
        else{
            return temp->data;
        }
    }
    return ceil;
}
